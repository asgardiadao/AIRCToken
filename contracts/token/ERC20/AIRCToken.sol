pragma solidity ^0.8.0;

import "./ERC20.sol";
import "../../access/Ownable.sol";

/**
 * @title AIRCToken
 * @dev Total tokens 14285700000
*/

contract AIRCToken is ERC20, Ownable{

  /**
   * @dev Initialize the contract, issue tokens
   */
  constructor() ERC20("Asgardia DAO", "AIRC") {
    _mint(msg.sender, 14285700000 ether);
  }

  /**
   * @dev Burn AIRC
   */
  function burnAIRC(uint256 amount) public onlyOwner returns (bool){
    _burn(msg.sender, amount);
    return true;
  }
}
